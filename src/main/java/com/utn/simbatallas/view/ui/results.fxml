<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane stylesheets="@../../../assets/css/style.css" xmlns="http://javafx.com/javafx/8.0.71"
            xmlns:fx="http://javafx.com/fxml/1">
    <GridPane>
        <columnConstraints>
            <ColumnConstraints halignment="RIGHT"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
        </rowConstraints>
        <MenuBar GridPane.columnIndex="0" GridPane.rowIndex="0">
            <Menu mnemonicParsing="false" text="Opciones">
                <MenuItem mnemonicParsing="false" text="Salir          "/>
            </Menu>
        </MenuBar>
        <GridPane GridPane.columnIndex="0" GridPane.rowIndex="1">
            <columnConstraints>
                <ColumnConstraints halignment="CENTER" percentWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
            </rowConstraints>
            <Label styleClass="title" text="RESULTADOS DE BATALLAS ANTERIORES" GridPane.columnIndex="0"
                   GridPane.rowIndex="0">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
            </Label>
        </GridPane>
        <GridPane GridPane.columnIndex="0" GridPane.rowIndex="2">
            <columnConstraints>
                <ColumnConstraints/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
                <RowConstraints/>
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <GridPane hgap="20.0" vgap="20.0">
                <columnConstraints>
                    <ColumnConstraints halignment="CENTER"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints/>
                    <RowConstraints/>
                    <RowConstraints/>
                    <RowConstraints/>
                    <RowConstraints/>
                    <RowConstraints/>
                    <RowConstraints/>
                    <RowConstraints/>
                </rowConstraints>
                <padding>
                    <Insets right="10.0"/>
                </padding>
                <Button fx:id="reload" onAction="#handleBtnReload" mnemonicParsing="false" text="Recargar"
                        GridPane.rowIndex="3"/>
                <Button fx:id="goBack" onAction="#handleBtnGoBack" mnemonicParsing="false" text="Volver"
                        GridPane.rowIndex="7"/>
            </GridPane>
            <TableView fx:id="tableResults" GridPane.columnIndex="1" GridPane.rowIndex="0">
                <columns >
                    <TableColumn fx:id="c21" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0"
                                 resizable="false" sortable="false" text="NRO"/>
                    <TableColumn fx:id="c22" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
                                 resizable="false" sortable="false" text="GANADOR"/>
                    <TableColumn fx:id="c23" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
                                 resizable="false" sortable="false" text="BATALLA"/>
                    <TableColumn fx:id="c24" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
                                 resizable="false" sortable="false" text="N° UNIDADES"/>
                    <TableColumn fx:id="c25" editable="false" maxWidth="140.0" minWidth="140.0" prefWidth="140.0"
                                 resizable="false" sortable="false" text="UNIDADES PERDIDAS"/>
                    <TableColumn fx:id="c26" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0"
                                 resizable="false" sortable="false" text="PERDEDOR"/>
                    <TableColumn fx:id="c27" editable="false" maxWidth="190.0" minWidth="190.0" prefWidth="190.0"
                                 resizable="false" sortable="false" text="UNIDADES DERROTADAS"/>
                </columns>
            </TableView>
        </GridPane>
        <Label text="Trabajo Práctico Primer Parcial | by Ignacio Casales" GridPane.columnIndex="0"
               GridPane.rowIndex="3">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Label>
    </GridPane>
</AnchorPane>
